{
  "name": "wutils",
  "version": "1.0.0",
  "description": "utils for windpress",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "compile:commonjs": "better-npm-run compile:commonjs",
    "compile:umdmin": "uglifyjs dist/wutils.js -m -o dist/wutils.min.js",
    "compile:umd": "better-npm-run compile:umd",
    "compile:es": "babel -d es/ src/",
    "compile": "npm run compile:commonjs && npm run compile:umd && npm run compile:umdmin && npm run compile:es",
    "build": "babel src -d lib",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint --fix src",
    "prepublish": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "compile:commonjs": {
      "command": "babel -d lib/ src/",
      "env": {
        "NODE_ENV": "commonjs"
      }
    },
    "compile:umd": {
      "command": "mkdirp dist/ && babel -o dist/wutils.js src/",
      "env": {
        "NODE_ENV": "umd"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-check-es2015-constants": "^6.7.2",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
    "babel-plugin-transform-es2015-block-scoping": "^6.7.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-plugin-transform-es2015-parameters": "^6.7.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "babel-plugin-transform-es2015-template-literals": "^6.6.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "0.0.8",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.5",
    "uglify-js": "^3.0.20"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
